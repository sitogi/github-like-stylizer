{
  "name": "github-style",
  "author": "sitogi",
  "description": "chrome extension for customize AWS CodeCommit CSS to like GitHub.",
  "version": "1.0.0",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://ap-northeast-1.console.aws.amazon.com/codesuite/codecommit/*"],
      "css": ["codecommit/style.css"]
    },
    {
      "matches": ["http://redmine.example.com/*"],
      "css": ["redmine/style.css"]
    }
  ]
}
